package com;

/**
 * 配置信息
 * deploy-config.xml
 */
public class Config {
    private String webDir;
    private String deployDir;
    private boolean init = false;

    public Config(String webDir, String deployDir) {
        this.webDir = webDir;
        this.deployDir = deployDir;
    }

    public Config(String webDir, String deployDir, boolean init) {
        this.webDir = webDir;
        this.deployDir = deployDir;
        this.init = init;
    }

    public String getWebDir() {
        return this.webDir;
    }

    public String getDeployDir() {
        return this.deployDir;
    }

    public String getInit() {
        return this.init;
    }

}